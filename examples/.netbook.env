# These variables can be used in your requests using {{variableName}} syntax

# Base API URL
baseUrl=https://jsonplaceholder.typicode.com

# Authentication token
token=your_bearer_token_here

# User ID for testing
userId=1

# Search parameters
searchUserId=2

# API version (if needed)
apiVersion=v1

# Timeout settings (in seconds)
timeout=30